files:
    "/sbin/appstart":
        mode: "000755"
        owner: webapp
        group: webapp
        content: |
            #!/usr/bin/env bash
            JAR_PATH=/var/app/current/application.jar

            # run app
            # don't need kill java. because new instance use.
            java -Dfile.encoding=UTF-8 -jar $JAR_PATH